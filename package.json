{
	"name": "wyreferee",
	"version": "6.3.0",
	"description": "Referee client",
	"author": {
		"name": "Wesley"
	},
	"keywords": [
		"osu",
		"irc",
		"axs",
		"bancho",
		"bancho.js",
		"referee client"
	],
	"main": "main.js",
	"private": true,
	"scripts": {
		"gh-publish-windows": "npm run build:prod && electron-builder -p always",
		"gh-publish-linux": "npm run build:prod && electron-builder build --linux -p always",
		"gh-publish-mac": "npm run build:prod && electron-builder build --mac -p always",
		"gh-publish-mac-arm": "npm run build:prod && electron-builder build --mac --arm64 -p always",
		"postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
		"postinstall:web": "node postinstall-web",
		"postinstall:electron": "node postinstall",
		"ng": "ng",
		"start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
		"build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
		"build:dev": "npm run build -- -c dev",
		"build:prod": "npm run build -- -c production",
		"ng:serve": "ng serve",
		"ng:serve:web": "npm run postinstall:web && ng serve -o",
		"electron:serve-tsc": "tsc -p tsconfig-serve.json",
		"electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
		"electron:local": "npm run build:prod && electron .",
		"electron:linux": "npm run build:prod && electron-builder build --linux --publish never",
		"electron:windows": "npm run build:prod && electron-builder build --windows --publish never",
		"electron:mac": "npm run build:prod && electron-builder build --mac --publish never",
		"test": "npm run postinstall:web && ng test",
		"e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
		"lint": "ng lint"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "12.1.3",
		"@angular-devkit/build-angular": "12.2.17",
		"@angular-eslint/builder": "2.0.2",
		"@angular-eslint/eslint-plugin": "2.0.2",
		"@angular-eslint/eslint-plugin-template": "2.0.2",
		"@angular-eslint/template-parser": "2.0.2",
		"@angular/cdk": "11.2.13",
		"@angular/cli": "12.2.17",
		"@angular/common": "12.2.16",
		"@angular/compiler": "12.2.16",
		"@angular/compiler-cli": "12.2.16",
		"@angular/core": "12.2.16",
		"@angular/forms": "12.2.16",
		"@angular/language-service": "12.2.16",
		"@angular/platform-browser": "12.2.16",
		"@angular/platform-browser-dynamic": "12.2.16",
		"@angular/router": "12.2.16",
		"@types/howler": "^2.1.2",
		"@types/jasmine": "3.3.16",
		"@types/jasminewd2": "2.0.6",
		"@types/mocha": "5.2.7",
		"@types/node": "^15.12.5",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"codelyzer": "^6.0.0",
		"core-js": "3.6.5",
		"electron": "^13.1.4",
		"electron-builder": "22.10.5",
		"electron-reload": "1.5.0",
		"eslint": "^7.8.1",
		"eslint-plugin-import": "^2.22.0",
		"howler": "^2.1.3",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.3.17",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~3.3.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"mocha": "6.2.0",
		"npm-run-all": "4.1.5",
		"rxjs": "6.5.5",
		"ts-node": "8.3.0",
		"tslib": "^2.1.0",
		"typescript": "~4.2.0",
		"wait-on": "5.0.1",
		"webdriver-manager": "12.1.8",
		"zone.js": "~0.11.4"
	},
	"engines": {
		"node": ">=10.9.0"
	},
	"dependencies": {
		"@angular/animations": "^12.2.16",
		"@angular/material": "^10.2.0",
		"bancho.js": "^0.9.7",
		"electron-log": "^3.0.8",
		"electron-store": "^5.0.0",
		"electron-updater": "^4.1.2",
		"ngx-clipboard": "^12.3.1",
		"ngx-virtual-scroller": "^3.0.3",
		"semver": "^7.3.2"
	}
}
